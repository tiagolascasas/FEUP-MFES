class EdgeType

types
  public TravelType = <ANY> | <CAR> | <BUS> | <PLANE> | <TRAIN> | <FERRY>;

instance variables
  public travelType:TravelType;
  public time:nat1;
  public distance:nat1;
  public price:real;
  
operations

 -- constructor of EdgeType
	public EdgeType : TravelType * nat1 * nat1 * real ==> EdgeType
	EdgeType(tt,tm,dist,p) == 
	(
		travelType := tt;
		time := tm;
		distance := dist;	
		price := p;
		return self;
	)
	pre tt <> <ANY>;

end EdgeType
